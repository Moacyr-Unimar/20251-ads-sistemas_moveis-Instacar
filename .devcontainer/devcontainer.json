{
    "name": "Projeto Mobile",
    "dockerComposeFile": [
      "../docker-compose.yml"
    ],
    "service": "instacar-api",
    "workspaceFolder": "/workspace",
    "settings": {
      "terminal.integrated.defaultProfile.linux": "bash"
    },
    "extensions": [
      "dbaeumer.vscode-eslint",
      "dart-code.flutter",
      "esbenp.prettier-vscode"
    ],
    "postCreateCommand": "npm install --prefix instacar-api && flutter pub get --directory instacar",
    "remoteUser": "root"
  }